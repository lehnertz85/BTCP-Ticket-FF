{
  "manifest_version":2,
  "name":"BTCP Ticker",
  "description":"BTCP Price Monitor and other utilities around BTCP",
  "icons": {
    "16": "images/btcp_icon_16.png",
    "48": "images/btcp_icon_48.png",
    "128": "images/btcp_icon_128.png"
  },
  "version":"2.0.1",
  "background":{
    "page":"background.html"
  },
  "content_security_policy": "script-src 'self' https://files.coinmarketcap.com/ https://ajax.googleapis.com/; object-src 'self'",
  "browser_action":{
    "default_icon":"icon.png",
    "default_popup":"popup.html"
  },
  "author":"Will Lehnertz",
  "web_accessible_resources":[
    "icon.png"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "permissions":[
    "notifications",
    "storage",
    "https://api.coinmarketcap.com/",
    "http://www.ecb.europa.eu/"
  ]
}